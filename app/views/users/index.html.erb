<h1>Listing Users</h1>

<table class="table table-striped">
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to (image_tag user.avatar(:thumb)), user_path(user) %></td>
        <td><%= link_to user.name, user_path(user) %></td>
        <td><%= %></td>
        <td><%= %></td>
        <td><%= %></td>
      </tr>
    <% end %>
  </tbody>
  </table>
</body>

